{
    "workbench.colorTheme": "Ayu Dark",
    "files.autoSave": "onFocusChange",
    "workbench.iconTheme": "material-icon-theme",
    "editor.linkedEditing": true,
    "liveServer.settings.donotShowInfoMsg": true,
    "editor.wordWrap": "on",
    "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
    "editor.tokenColorCustomizations": {
        "comments": "#d4922f"
      },
      "workbench.colorCustomizations": {
        "editorGroupHeader.tabsBackground": "#2c2c54",
        "activityBar.background": "#1d1d4a",
        "activityBar.activeBackground": "#ffebeb",
        "sideBar.background": "#141422",
        "minimap.background": "#141422",
        "tab.activeBackground": "#1d1d4a",
        "terminal.border": "#2c2c54",
        "terminal.background": "#09091a",
        "statusBar.background": "#07071e",
        "statusBar.border": "#0c1835",
        "scrollbarSlider.background": "#474787",
        "scrollbarSlider.hoverBackground": "#706fd3"
      },
      "bracketPairColorizer.depreciation-notice": false,
      "security.workspace.trust.untrustedFiles": "open"
}


